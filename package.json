{
  "name": "weather-app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint . --ignore-pattern 'android/'",
    "start": "react-native start",
    "format": "prettier --loglevel silent . --write",
    "diff": "npm run types && python scripts/diff.py",
    "diff_bypass": "python scripts/diff.py",
    "dump": "python scripts/codebase_dumper.py",
    "dump_lite": "python scripts/codebase_dumper_lite.py",
    "bundle": "node scripts/react-native-bundle-visualizer.js",
    "apk": "react-native run-android --mode=release",
    "doctor": "react-native doctor",
    "arch-build": "cd android && gradlew.bat assembleRelease -PreactNativeArchitectures=armeabi-v7a",
    "arch-install": "adb install -f android/app/build/outputs/apk/release/app-release.apk",
    "arch": "npm run arch-build && npm run arch-install",
    "build-both": "npm run arch && npm start",
    "types": "tsc --noEmit",
    "checks": "npm run types && npm run lint && npm run format",
    "test": "jest",
    "generate-icons": "python scripts/generate_icons.py"
  },
  "dependencies": {
    "@react-native-firebase/analytics": "^21.13.0",
    "@react-native-firebase/app": "^21.13.0",
    "@react-native-picker/picker": "^2.11.0",
    "@react-navigation/native": "^7.0.18",
    "@react-navigation/native-stack": "^7.3.2",
    "i18next": "^24.2.3",
    "nativewind": "^4.1.23",
    "react": "18.3.1",
    "react-i18next": "^15.4.1",
    "react-native": "0.76.7",
    "react-native-bootsplash": "^6.3.4",
    "react-native-chart-kit": "^6.12.0",
    "react-native-fast-image": "^8.6.3",
    "react-native-geolocation-service": "^5.3.1",
    "react-native-gesture-handler": "^2.25.0",
    "react-native-google-mobile-ads": "^15.0.0",
    "react-native-mmkv": "^3.2.0",
    "react-native-reanimated": "^3.17.5",
    "react-native-safe-area-context": "^5.4.0",
    "react-native-screens": "^4.9.2",
    "react-native-svg": "^15.11.2",
    "react-native-toast-message": "^2.3.0",
    "react-native-vector-icons": "^10.2.0",
    "tailwindcss": "^3.4.17"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@eslint/js": "^9.22.0",
    "@react-native-community/cli": "15.0.1",
    "@react-native-community/cli-platform-android": "15.0.1",
    "@react-native-community/cli-platform-ios": "15.0.1",
    "@react-native/babel-preset": "0.76.7",
    "@react-native/eslint-config": "0.76.7",
    "@react-native/metro-config": "0.76.7",
    "@react-native/typescript-config": "0.76.7",
    "@testing-library/react-native": "^13.2.0",
    "@types/jest": "^29.5.14",
    "@types/react": "^18.2.6",
    "@types/react-native-vector-icons": "^6.4.18",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.57.1",
    "eslint-import-resolver-typescript": "^4.3.4",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.37.4",
    "globals": "^16.0.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "18.3.1",
    "source-map-explorer": "^2.5.3",
    "typescript": "5.0.4",
    "typescript-eslint": "^8.26.0"
  },
  "engines": {
    "node": ">=18"
  }
}
