{
  "name": "easyweather",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "expo lint --fix",
    "types": "tsc --noEmit",
    "checks": "npm run types && npm run lint",
    "apk": "expo run:android --variant=release",
    "bundle:android": "cd android && .\\gradlew.bat bundleRelease && cd ..",
    "release:prebuild": "expo prebuild -p android",
    "release:assemble": "cd android && .\\gradlew.bat assembleRelease && cd ..",
    "release:clear-data": "adb shell pm clear com.uwayss.easyweather",
    "release:install": "adb install -f android\\app\\build\\outputs\\apk\\release\\app-release.apk",
    "release:start-app": "adb shell am start -n com.uwayss.easyweather/com.uwayss.easyweather.MainActivity",
    "release": "npm run release:prebuild && npm run release:assemble && npm run release:clear-data && npm run release:install && npm run release:start-app",
    "prebuild": "expo prebuild -p android",
    "deps": "expo install --fix",
    "commit": "npm run checks && ups diff"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "expo": "53.0.20",
    "expo-build-properties": "~0.14.8",
    "expo-dev-client": "~5.2.4",
    "expo-image": "~2.4.0",
    "expo-linking": "~7.1.7",
    "expo-location": "~18.1.6",
    "expo-router": "~5.1.4",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-system-ui": "~5.0.10",
    "expo-updates": "^0.28.17",
    "i18next": "^25.2.0",
    "nativewind": "^4.1.23",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-i18next": "^15.5.1",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-web": "~0.20.0",
    "tailwindcss": "^3.4.17"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@expo/config-plugins": "~10.1.1",
    "@types/react": "~19.0.10",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~9.2.0",
    "typescript": "~5.8.3"
  },
  "private": true
}
